    "def trailingZero(n):\n",
    "  ans = 0\n",
    "  i = 5\n",
    "  while n/i > 1:\n",
    "    ans += int(n/i)\n",
    "    i = i * 5\n",
    "    \n",
    "  return int(ans)\n",
    "\n",
    "n = int(input())\n",
    "print(trailingZero(n))"
   ]
  }
